            <WordsStyle name="DELIMITERS5" fgColor="000000" bgColor="FFFFFF" fontName="Fixedsys" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS6" fgColor="000000" bgColor="FFFFFF" fontName="Fixedsys" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS7" fgColor="000000" bgColor="FFFFFF" fontName="Fixedsys" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS8" fgColor="000000" bgColor="FFFFFF" fontName="Fixedsys" fontStyle="0" nesting="0" />
        </Styles>
    </UserLang> 
</NotepadPlus>
